<Window x:Class="MeropriatiyaKrush.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MeropriatiyaKrush"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="45"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Frame Name="Fr" Source="Pages/MainPage.xaml" NavigationUIVisibility="Hidden" Grid.Row="1" Navigated="MainFrame_OnNavigated" />
        <Button Name="Backbtn" Content="Назад" Grid.Row="0" HorizontalAlignment="Left"  Width="71" Click="Backbtn_Click" />

    </Grid>
</Window>
